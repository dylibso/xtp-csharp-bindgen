// Note: run `go doc -all` in this package to see all of the types and functions available.
// ./pdk.gen.go contains the domain types from the host where your plugin will run.
package main

<% if (Object.values(schema.schemas).length > 0) { %>
	
	<% schema.exports.forEach(ex => { %>
	<% if (hasComment(ex)) -%>	
	// <%= formatGoBlockComment(ex.description) %>
	<% if (hasComment(ex.input)) { -%>
	// It takes <%= toGolangType(ex.input) %> as input (<%= formatGoComment(ex.input.description) %>)
	<% } -%>
	<% if (hasComment(ex.output)) { -%>
	// And returns <%= toGolangType(ex.output) %> (<%= formatGoComment(ex.output.description) %>)
	<% } -%>
	<% -%>
	func <%= ex.name %>(<%- ex.input ? `input ${toGolangType(ex.input)}` : null %>) <%- ex.output ? `(${toGolangType(ex.output)}, error)` : "error" %> {
		<% if (featureFlags['stub-with-code-samples'] && codeSamples(ex, 'go')[0]) { -%>
			<%- codeSamples(ex, 'go')[0].source %>
		<% } else { -%>
			// TODO: fill out your implementation here
			panic("Function not implemented.")
		<% } -%>
	}
	<% }) %>

<% } %>
	
	
	